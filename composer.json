{
  "name": "menatwork/contao-syncaccclient-bundle",
  "type": "contao-bundle",
  "description": "Synchronize user and member",
  "keywords": [
    "synchronization",
    "communication",
    "framework"
  ],
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Stefan Heimes",
      "email": "heimes@men-at-work.de",
      "homepage": "http://www.men-at-work.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^8.0",
    "contao/core-bundle": "^4.13.0",
    "menatwork/ctocommunication": "^3.0.2 || ^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "SyncAccClientBundle\\": "src"
    }
  },
  "extra": {
    "contao-manager-plugin": "SyncAccClientBundle\\ContaoManager\\Plugin"
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true
    }
  }
}
